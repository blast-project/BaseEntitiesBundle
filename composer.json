    "description": "Base behaviors for Doctrine Entities in Libre Informatique Symfony2 projects",
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "2.7.*",
        "knplabs/doctrine-behaviors": "~1.1",
        "libre-informatique/user-bundle": "dev-master"
    },
    "license": "GNU-GPL",
    "keywords": [
        "e-venement",
        "libio",
        "symfony"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoBaseEntitiesBundle",
    "authors": [
        {
            "name": "Baptiste SIMON",
            "email": "baptiste.simon@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Librinfo\\BaseEntitiesBundle\\": ""
        }
